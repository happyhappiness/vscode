+        if(-f $pidfile) {
+            open(PID, "<$pidfile");
+            $pid2 = 0 + <PID>;
+            close(PID);
+            if(kill(0, $pid2)) {
+                # make sure this pid is alive, as otherwise it is just likely
+                # to be the _previous_ pidfile or similar!
+                last;
+            }
+        }
+        sleep(1);
+    }
+
+    return ($child, $pid2);
