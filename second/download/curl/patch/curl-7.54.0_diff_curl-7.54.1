+  curl_easy_setopt(curl, CURLOPT_PROXY, "http://127.0.0.1:80");
+  curl_easy_setopt(curl, CURLOPT_HTTPPROXYTUNNEL, 1L);
+  curl_easy_perform(curl);
+}
+.fi
