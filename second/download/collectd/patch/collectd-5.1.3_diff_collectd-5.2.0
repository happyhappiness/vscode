+
+	if (include_unit_id)
+		ssnprintf (buffer, buffer_size, "%s-%"PRIu32,
+				refclock_names[refclock_id], unit_id);
+	else
+		sstrncpy (buffer, refclock_names[refclock_id], buffer_size);
+
+	return (0);
+} /* int ntpd_get_name_refclock */
